<Window x:Class="PharmY.Window2"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PharmY"
        mc:Ignorable="d"
        Title="Hospital" Height="300" Width="300">
    <Grid>
        <Label x:Name="lblbarcode" Content="Barcode" HorizontalAlignment="Left" Margin="33,98,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="edtbarcode" HorizontalAlignment="Left" Height="23" Margin="120,101,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <ComboBox HorizontalAlignment="Left" Margin="128,54,0,0" VerticalAlignment="Top" Width="87" SelectionChanged="ComboBox_SelectionChanged"/>
        <DatePicker HorizontalAlignment="Left" Margin="41,15,0,0" VerticalAlignment="Top"/>
        <Label Content="Active Ingridient" HorizontalAlignment="Left" Margin="10,138,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="edtactiveingridient" HorizontalAlignment="Left" Height="23" Margin="120,138,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <Label Content="Quantity" HorizontalAlignment="Left" Margin="22,169,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="edtquantity" HorizontalAlignment="Left" Height="23" Margin="120,172,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <Label Content="out where" HorizontalAlignment="Left" Margin="27,54,0,0" VerticalAlignment="Top"/>
    </Grid>
</Window>
